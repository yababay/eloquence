{
	"name": "eloquence",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"deploy:rsync": "vite build && rsync -ar build/ $npm_package_config_ssh_host:/var/www/$npm_package_config_www_prefix.$npm_package_name",
		"deploy:gh-pages": "npm run build && gh-pages -d build -t true",
		"docker:build": "npm run build && docker-compose build",
		"docker:start": "docker-compose up",
		"docker:cli": "docker exec -it mongodb /usr/bin/mongo eloquence",
		"test": "vitest"
	},
	"config": {
		"ssh_host": "random-history",
		"www_prefix": "me.yababay"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.27.4",
		"sass": "^1.69.5",
		"svelte": "^4.0.5",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"gh-pages": "^6.1.0",
		"vitest": "^0.34.0"
	},
	"type": "module",
	"dependencies": {
		"@yababay67/sveltekit-components": "^1.0.7"
	}
}
